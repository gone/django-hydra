{% extends "base.html" %}

{% block title %}
    {% translate "Page not found" %}
{% endblock title %}

{% block content %}
    <h1>{% translate "Page not found" %}</h1>
    <p>
        {% if exception %}
            {{ exception }}
        {% else %}
            {% translate "This is not the page you were looking for." %}
        {% endif %}
    </p>
{% endblock content %}
